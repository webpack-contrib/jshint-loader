{
  "name": "jshint-loader",
  "version": "0.8.4",
  "description": "A JSHint loader for webpack",
  "license": "MIT",
  "repository": "webpack-contrib/jshint-loader",
  "author": "Tobias Koppers @sokra",
  "homepage": "https://github.com/webpack-contrib/jshint-loader",
  "bugs": "https://github.com/webpack-contrib/jshint-loader/issues",
  "bin": "",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 6.9.0 <7.0.0 || >= 8.9.0"
  },
  "scripts": {
    "commitlint": "commitlint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint --cache src test",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "lint-staged": "lint-staged",
    "release": "standard-version",
    "release:ci": "conventional-github-releaser -p angular",
    "release:validate": "commitlint --from=$(git describe --tags --abbrev=0) --to=$(git rev-parse HEAD)",
    "security": "nsp check",
    "test": "jest --env=node",
    "test:watch": "jest --watch",
    "test:coverage": "jest --env=node --collectCoverageFrom='src/**/*.js' --coverage",
    "ci:lint": "npm run lint && npm run security",
    "ci:test": "npm run test -- --runInBand",
    "ci:coverage": "npm run test:coverage -- --runInBand",
    "defaults": "webpack-defaults"
  },
  "files": [
    "lib/"
  ],
  "peerDependencies": {
    "jshint": "^2.5.0",
    "webpack": "^4.5.0"
  },
  "dependencies": {
    "@webpack-contrib/schema-utils": "^1.0.0-beta.0",
    "loader-utils": "^1.1.0",
    "lodash": "4.17.5",
    "rcloader": "^0.2.2",
    "shelljs": "0.7.6",
    "strip-json-comments": "0.1.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@webpack-contrib/eslint-config-webpack": "^2.0.4",
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "conventional-github-releaser": "^2.0.2",
    "cross-env": "^5.1.4",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jshint": "^2.9.2",
    "lint-staged": "^7.0.4",
    "memory-fs": "^0.4.1",
    "mocha": "^3.0.2",
    "nsp": "^3.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "should": "^10.0.0",
    "sinon": "^1.17.5",
    "standard-version": "^4.3.0",
    "webpack": "^4.5.0",
    "webpack-defaults": "^2.3.0"
  },
  "keywords": [
    "webpack"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
